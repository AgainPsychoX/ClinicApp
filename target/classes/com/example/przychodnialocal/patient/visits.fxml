<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="contentAnchorPane" minHeight="0.0" minWidth="0.0" prefHeight="914.0" prefWidth="1024.0" style="-fx-background-color: #E4E4E4;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.przychodnialocal.patient.VisitsController">
   <children>
      <TableView fx:id="notificationsTableView" layoutX="11.0" layoutY="190.0" prefHeight="627.0" prefWidth="1002.0">
         <columns>
            <TableColumn prefWidth="136.0" text="Termin" />
            <TableColumn prefWidth="139.0" text="Specjalizacja" />
            <TableColumn prefWidth="273.0" text="Lekarz" />
            <TableColumn prefWidth="453.0" text="..." />
         </columns>
      </TableView>
      <Button fx:id="createVisitButton" layoutX="14.0" layoutY="114.0" mnemonicParsing="false" onAction="#createVisit" prefHeight="62.0" prefWidth="315.0" style="-fx-background-color: #4D89FF;" text="Umów wizytę" textFill="WHITE" />
      <HBox alignment="TOP_CENTER" layoutX="650.0" layoutY="61.0" prefHeight="47.0" prefWidth="363.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Wyszukaj:" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
            </Text>
            <TextField fx:id="searchNotificationsTextField" alignment="TOP_RIGHT" />
         </children>
      </HBox>
      <Label fx:id="visitsLabel" layoutX="20.0" layoutY="76.0" prefHeight="24.0" prefWidth="201.0" text="Nadchodzące wizyty" />
      <Button fx:id="oldVisitsButton" layoutX="355.0" layoutY="829.0" mnemonicParsing="false" onAction="#showOldVisits" prefHeight="62.0" prefWidth="315.0" style="-fx-background-color: #4D89FF;" text="Wizyty archiwalne" textFill="WHITE" />
      <Button fx:id="returnButton" disable="true" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#showCurrentVisits" prefHeight="62.0" prefWidth="315.0" style="-fx-background-color: #4D89FF;" text="Powrót do wizyt" textFill="WHITE" visible="false" />
   </children>
</AnchorPane>
