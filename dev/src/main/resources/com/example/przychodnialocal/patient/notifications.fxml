<AnchorPane fx:id="contentAnchorPane" minHeight="0.0" minWidth="0.0" prefHeight="366.0" prefWidth="436.0"
            style="-fx-background-color: #E4E4E4;" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.przychodnialocal.patient.NotificationsController">
    <children>
        <TableView fx:id="notificationsTableView" layoutX="36.0" layoutY="61.0" prefHeight="724.0" prefWidth="1002.0">
            <columns>
                <TableColumn prefWidth="136.0" text="Data"/>
                <TableColumn prefWidth="139.0" text="Kategoria"/>
                <TableColumn prefWidth="273.0" text="Od"/>
                <TableColumn prefWidth="453.0" text="Treść"/>
            </columns>
        </TableView>
        <Button fx:id="unreadButton" layoutX="183.0" layoutY="832.0" mnemonicParsing="false" prefHeight="62.0"
                prefWidth="315.0" style="-fx-background-color: #4D89FF;" text="Oznacz jako nieprzeczytane"
                textFill="WHITE"/>
        <Button fx:id="deleteNotificationsButton" layoutX="643.0" layoutY="832.0" mnemonicParsing="false"
                prefHeight="62.0" prefWidth="315.0" style="-fx-background-color: #4D89FF;" text="Usuń"
                textFill="WHITE"/>
        <HBox alignment="TOP_CENTER" layoutX="696.0" layoutY="14.0" prefHeight="47.0" prefWidth="363.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Wyszukaj:" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets top="5.0"/>
                    </HBox.margin>
                </Text>
                <TextField fx:id="searchNotificationsTextField" alignment="TOP_RIGHT"/>
            </children>
        </HBox>
    </children>
</AnchorPane>

