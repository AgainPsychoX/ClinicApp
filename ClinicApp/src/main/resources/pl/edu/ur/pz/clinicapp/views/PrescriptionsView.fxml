<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox fx:id="vBox" prefHeight="540.0" prefWidth="600.0" spacing="8.0" stylesheets="@../common.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.ur.pz.clinicapp.views.PrescriptionsView">
   <children>
      <HBox alignment="CENTER_LEFT">
         <children>
            <HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Szukaj: " />
                  <TextField fx:id="searchTextField" />
               </children>
            </HBox>
         </children>
      </HBox>
      <TableView fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
        <columns>
          <TableColumn fx:id="patientCol" maxWidth="200.0" prefWidth="133.0" text="Pacjent" />
            <TableColumn fx:id="contentCol" maxWidth="223.0" prefWidth="212.0" text="Treść" />
            <TableColumn fx:id="dateCol" maxWidth="200.0" minWidth="0.0" prefWidth="108.0" text="Data" />
            <TableColumn fx:id="doctorCol" maxWidth="236.0" prefWidth="114.0" text="Lekarz" />
        </columns>
      </TableView>
      <HBox alignment="CENTER_LEFT">
         <children>
            <HBox alignment="CENTER" spacing="8.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="markUnreadButton11" mnemonicParsing="false" prefWidth="200.0" styleClass="my-primary-button" text="Dodaj receptę">
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </Button>
                  <Button fx:id="markUnreadButton" disable="true" mnemonicParsing="false" prefWidth="200.0" styleClass="my-primary-button" text="Edytuj lub anuluj">
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </Button>
                  <Button fx:id="markUnreadButton1" disable="true" mnemonicParsing="false" prefWidth="200.0" styleClass="my-primary-button" text="Przejdź do e-recepty (IKP)">
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</VBox>
