<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="540.0" prefWidth="600.0" spacing="8.0" stylesheets="@../common.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.ur.pz.clinicapp.views.AccountsView">
   <children>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Typ konta: " />
            <ComboBox prefWidth="150.0" promptText="Lekarze" />
            <HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Szukaj: " />
                  <TextField fx:id="searchTextField" />
               </children>
            </HBox>
         </children>
      </HBox>
      <TableView fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" >
        <columns>
          <TableColumn maxWidth="200.0" prefWidth="100.0" text="Imię" />
          <TableColumn maxWidth="200.0" prefWidth="100.0" text="Nazwisko" />
            <TableColumn maxWidth="200.0" prefWidth="100.0" text="Specjalizacja" />
            <TableColumn maxWidth="200.0" prefWidth="100.0" text="E-mail" />
            <TableColumn maxWidth="200.0" prefWidth="100.0" text="Telefon" />
        </columns>
      </TableView>
      <HBox alignment="CENTER_LEFT">
         <children>
            <HBox alignment="CENTER" spacing="8.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="markUnreadButton11" mnemonicParsing="false" prefWidth="200.0" text="Dodaj">
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </Button>
                  <Button fx:id="markUnreadButton1" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Edytuj">
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </Button>
                  <Button fx:id="markUnreadButton" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Usuń">
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</VBox>
