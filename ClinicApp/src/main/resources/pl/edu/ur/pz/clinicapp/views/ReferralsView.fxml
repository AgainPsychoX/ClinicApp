<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="vBox" prefHeight="540.0" prefWidth="600.0" spacing="8.0" stylesheets="@../common.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.ur.pz.clinicapp.views.ReferralsView">
   <children>
      <HBox alignment="CENTER_LEFT">
         <children>
            <HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Szukaj: " />
                  <TextField fx:id="searchTextField" />
               </children>
            </HBox>
         </children>
      </HBox>
      <TableView fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
        <columns>
          <TableColumn fx:id="patientCol" maxWidth="200.0" prefWidth="133.0" text="Pacjent" />
            <TableColumn fx:id="specCol" maxWidth="200.0" prefWidth="111.0" text="Specjalność" />
            <TableColumn fx:id="dateCol" maxWidth="200.0" prefWidth="123.0" text="Data skierowania" />
            <TableColumn fx:id="doctorCol" maxWidth="200.0" prefWidth="126.0" text="Lekarz skierowujący" />
            <TableColumn fx:id="commetsCol" maxWidth="200.0" prefWidth="74.0" text="Uwagi" />
        </columns>
      </TableView>
      <HBox alignment="CENTER_LEFT">
         <children>
            <HBox alignment="CENTER" spacing="8.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="markUnreadButton11" mnemonicParsing="false" prefWidth="200.0" text="Dodaj skierowanie">
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </Button>
                  <Button fx:id="markUnreadButton" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Edytuj lub anuluj">
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </Button>
                  <Button fx:id="markUnreadButton1" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Przejdź do e-skierowania (IKP)">
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</VBox>
